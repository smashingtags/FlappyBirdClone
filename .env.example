# Environment Variables for React Native Build Pipeline
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# Android Signing Configuration
ANDROID_KEYSTORE_FILE=my-release-key.keystore
ANDROID_KEY_ALIAS=my-key-alias
ANDROID_KEYSTORE_PASSWORD=your_keystore_password
ANDROID_KEY_PASSWORD=your_key_password

# iOS Signing Configuration
IOS_TEAM_ID=YOUR_TEAM_ID
IOS_BUNDLE_ID=com.yourcompany.flappybirdclone
IOS_PROVISIONING_PROFILE_UUID=your_provisioning_profile_uuid
IOS_CERTIFICATE_NAME=iPhone Distribution: Your Company Name

# Fastlane Configuration
FASTLANE_APPLE_APPLICATION_SPECIFIC_PASSWORD=your_app_specific_password
FASTLANE_SESSION=your_fastlane_session
MATCH_PASSWORD=your_match_password

# App Store Connect
APP_STORE_CONNECT_API_KEY_ID=your_api_key_id
APP_STORE_CONNECT_API_ISSUER_ID=your_issuer_id
APP_STORE_CONNECT_API_KEY_CONTENT=your_api_key_content

# Google Play Console
GOOGLE_PLAY_SERVICE_ACCOUNT_JSON=path_to_service_account.json
GOOGLE_PLAY_PACKAGE_NAME=com.yourcompany.flappybirdclone

# Build Configuration
NODE_ENV=production
BUILD_NUMBER=1
VERSION_NAME=1.0.0

# Docker Configuration
DOCKER_REGISTRY=your_docker_registry
DOCKER_IMAGE_TAG=latest

# CI/CD Configuration
GITHUB_TOKEN=your_github_token
SLACK_WEBHOOK_URL=your_slack_webhook_url